{
  "env": {
    "node": false,
    "commonjs": false,
    "es2021": false
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["jsdoc"],
  "rules": {
    "no-unused-vars": ["error", { "args": "none" }],
    "jsdoc/no-undefined-types": 1,
    "no-cond-assign": 0
  },
  "globals": {
    "__appRoot": "readonly"
  }
}
